{
  "manifest_version": 3,
  "name": "HiddenEye",
  "version": "1.0",
  "description": "Scans a webpage to find hidden endpoints and displays results in the popup.",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}